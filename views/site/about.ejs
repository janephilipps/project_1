		<div>
		function isFoggy(temp, dewPoint) {
     		if (temp > dewPoint) {
     			if (temp - dewPoint <= 4) {
     				return true;
     			} else if (dewPoint - temp <= 4) {
     				return true;
     			}
     		} else {
     			return false;
     		}
     	}
     	</div>
